@model List<PizzaPan.EntityLayer.Concrete.OurTeam>

    @foreach (var i in Model)
{

    <div class="col-lg-3 col-sm-6 mt-lg-5 mt-4">
        <div class="box16">
            <a href="#url"><img src="@i.OurTeamImageUrl" alt="" class="img-fluid radius-image" /></a>
            <div class="box-content">
                <h3 class="title"><a href="#url">@i.OurTeamName</a></h3>
                <span class="post">@i.OurTeamTitle</span>
                <ul class="social">
                    <li>
                        <a href="#" class="facebook">
                            <span class="fa fa-facebook-f"></span>
                        </a>
                    </li>
                    <li>
                        <a href="#" class="twitter">
                            <span class="fa fa-twitter"></span>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </div>


}


